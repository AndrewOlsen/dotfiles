! Adobe Source Code Pro
URxvt*font: xft:SauceCodePro Nerd Font:size=12,xft:Noto Emoji:style=Regular:antialias=false
URxvt.letterSpace: 1

Xft.autohint: false
Xft.dpi: 96
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: false
Xft.antialias: true
Xft.rgba: rgb

! Bitmap Font
!URxvt*font: xft:xos4 Terminus:size=14
!URxvt.letterSpace: 0

! inox
URxvt*perl-ext-common: default,matcher,clipboard,keyboard-select,url-select
URxvt*url-launcher:    /usr/bin/inox --incognito
URxvt*matcher.button:  1

URxvt*internalBorder: 20
URxvt*cursorBlink: 1

! Url stuff
URxvt.keysym.Meta-semicolon: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/inox --incognito
URxvt.url-select.underline: true

! Scrollbar
URxvt*scrollBar: off
URxvt.secondaryScreen: 1
URxvt.secondaryScroll: 0

! Clipboard enable
URxvt.keysym.Control-Shift-C: perl:clipboard:copy
URxvt.keysym.Control-Shift-V: perl:clipboard:paste
URxvt.iso14755: False

! Rofi
!rofi.color-enabled: true
!rofi.color-window: #393939, #393939, #3a9892
!rofi.color-normal: #393939, #ffffff, #393939, #3a9892, #ffffff
!rofi.color-active: #393939, #3a9892, #393939, #3a9892, #393939
!rofi.color-urgent: #393939, #f3843d, #393939, #3a9892, #ffc39c
